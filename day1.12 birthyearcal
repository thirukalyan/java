public class birthyearcal{
    public static void main(String[] args) {
        int currentYear = 2073;
        int eligibilityAge = 21;

        // Calculate the birth year
        int birthYear = currentYear - eligibilityAge;

        // Check if the birth year is more than 2060
        if (birthYear > 2060) {
            // Calculate the years between the person's vote-eligible year and 2023
            int yearsTo2023 = currentYear - 2023;
            System.out.println("Years between eligible year and 2023: " + yearsTo2023);
        } else {
            // Find the median of birth year and 2023
            int medianYear = (birthYear + 2023) / 2;
            System.out.println("Median of birth year and 2023: " + medianYear);
        }
    }
}
